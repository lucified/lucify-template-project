{
  "compilerOptions": {
    "outDir": "./lib/",
    "sourceMap": true,
    "pretty": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitThis": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "types": ["node"],
    "baseUrl": "./typings",
    "paths": {}
  }
}
