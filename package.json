{
  "name": "lucify-template-project",
  "version": "0.1.0",
  "devDependencies": {
    "@types/classnames": "^2.2.0",
    "@types/lodash": "^4.14.68",
    "@types/node": "^8.0.8",
    "@types/react": "^15.0.35",
    "@types/react-dom": "^15.5.1",
    "@types/react-helmet": "^5.0.3",
    "@types/react-redux": "^4.4.45",
    "@types/react-router-dom": "^4.0.5",
    "@types/react-router-redux": "^5.0.3",
    "atomic-s3": "^0.2.0",
    "autoprefixer": "^7.1.2",
    "awesome-typescript-loader": "^3.2.1",
    "babel-core": "^6.25.0",
    "babel-plugin-transform-regenerator": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "css-loader": "^0.28.4",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "husky": "^0.14.3",
    "json-loader": "^0.5.4",
    "lint-staged": "^4.0.1",
    "lucify-deploy-config": "^0.1.8",
    "lucify-notifier": "^0.1.17",
    "node-sass": "^4.5.3",
    "postcss-flexbugs-fixes": "^3.0.0",
    "postcss-loader": "^2.0.6",
    "prettier": "^1.5.2",
    "react-dev-utils": "^2.0.0",
    "react-error-overlay": "^1.0.9",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "tslint": "^5.5.0",
    "tslint-react": "^3.0.0",
    "typescript": "^2.4.1",
    "url-loader": "^0.5.9",
    "webpack": "^3.1.0",
    "webpack-dev-server": "^2.5.1",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "history": "^4.6.3",
    "lodash": "^4.17.2",
    "moment": "^2.17.1",
    "normalize.css": "^7.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.1.3",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "next",
    "redux": "^3.7.1",
    "reselect": "^3.0.0"
  },
  "scripts": {
    "start": "webpack-dev-server --config=webpack.config.dev.js --port 3000 --host 0.0.0.0 --progress --colors",
    "clean": "rm -rf dist lib build",
    "build": "yarn run clean && webpack --config=webpack.config.js",
    "build-dev": "yarb run clean && webpack --config=webpack.config.dev.js --progress --colors",
    "deploy-and-notify": "lucify-notifier yarn run deploy",
    "deploy": "yarn run build && yarn run upload",
    "deploy-chatops": "[ \"$AWS_TEMPORARY\" ] && yarn run deploy-and-notify || echo 'Skipping deployment'",
    "upload": "echo \"No upload script specified\" && exit 0",
    "compile": "tsc",
    "lint": "tslint -t verbose 'src/**/*.ts' 'src/**/*.tsx' 'src/**/*.js'",
    "prettify": "prettier --parser typescript --single-quote --trailing-comma all --write \"src/**/*.ts?(x)\"",
    "lint-staged-stash": "git stash save --keep-index 'lint-staged' && touch .didstash || rm .didstash || true",
    "lint-staged-stash-pop": "test -f .didstash && rm .didstash && git stash pop || true",
    "precommit": "lint-staged"
  },
  "engines": {
    "node": "^8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "lint-staged": {
    "*.js?(x)": [
      "lint-staged-stash",
      "prettier --single-quote --trailing-comma all --write",
      "git add",
      "lint-staged-stash-pop"
    ],
    "*.ts?(x)": [
      "lint-staged-stash",
      "prettier --parser typescript --single-quote --trailing-comma all --write",
      "git add",
      "lint-staged-stash-pop"
    ]
  }
}
